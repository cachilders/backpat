{
  "name": "backpat",
  "version": "0.5.0",
  "description": "A simple tool for high-fiving your tech stack",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc mocha",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "watch:test": "mocha:watch",
    "lint": "eslint src",
    "validate": "npm-run-all --parallel lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cachilders/backpat.git"
  },
  "author": "Casey A. Childers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cachilders/backpat/issues"
  },
  "homepage": "https://github.com/cachilders/backpat#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "coveralls": "^2.11.15",
    "eslint": "^3.8.1",
    "eslint-plugin-flowtype": "^2.25.0",
    "mocha": "^3.1.2",
    "npm-run-all": "^3.1.2",
    "nyc": "^10.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "functions": 90,
    "lines": 90,
    "statements": 90,
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "dependencies": {}
}
